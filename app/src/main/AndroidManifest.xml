<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wislight.parkmanage">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".model.home.MainActivity"></activity>
        <activity android:name=".model.loginRegiste.ForgetActivity" />
        <activity android:name=".model.loginRegiste.LoginActivity">
        </activity>
        <activity android:name=".model.loginRegiste.NewPasswordActivity" />
        <activity android:name=".model.loginRegiste.RegisterActivity" />
        <activity android:name=".model.loginRegiste.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>